{
  "connection": "default",
  "collectionName": "note",
  "info": {
    "name": "note",
    "description": "Note"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "archive": {
      "type": "boolean"
    },
    "delete": {
      "type": "boolean"
    },
    "color": {
      "required": true,
      "type": "string",
      "default": "#fff"
    },
    "user": {
      "required": true,
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "tags": {
      "type": "json"
    }
  }
}