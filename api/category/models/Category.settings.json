{
  "connection": "default",
  "collectionName": "category",
  "info": {
    "name": "category",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "user": {
      "model": "user",
      "via": "categories",
      "plugin": "users-permissions"
    },
    "notes": {
      "collection": "note",
      "via": "category"
    }
  }
}